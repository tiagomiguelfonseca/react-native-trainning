{
  "name": "InstagramClone",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --mode='devDebug' && npm run checkFlipper",
    "androidQA": "react-native run-android --mode='qaDebug' && npm run checkFlipper",
    "androidUAT": "react-native run-android --mode='uatDebug' && npm run checkFlipper",
    "androidProd": "react-native run-android --mode='prodDebug' && npm run checkFlipper",
    "ios": "react-native run-ios --scheme 'DEV' --udid 6D5C8268-4115-4B62-B21E-672D4F1043E5 && npm run checkFlipper",
    "iosQA": "react-native run-ios --scheme 'QA' && npm run checkFlipper",
    "iosUAT": "react-native run-ios --scheme 'UAT' && npm run checkFlipper",
    "iosProd": "react-native run-ios --scheme 'PROD' && npm run checkFlipper",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "checkFlipper": "sh './src/scripts/flipper.sh'",
    "changeiOSIdentifier": "sh './src/scripts/changeiOSIdentifier.sh'",
    "changeAndroidPackageName": "sh './src/scripts/changeAndroidPackageName.sh'",
    "sonarScan": "node ./src/scripts/sonarqubeScan.js",
    "podInstall": "cd ios && pod install && cd ..",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/netinfo": "9.3.7",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-navigation/bottom-tabs": "6.5.7",
    "@react-navigation/drawer": "6.6.2",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "1.3.4",
    "moment": "2.29.4",
    "patch-package": "^8.0.0",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-config": "1.5.0",
    "react-native-device-info": "10.4.0",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-flipper": "0.184.0",
    "react-native-gesture-handler": "2.9.0",
    "react-native-reanimated": "3.4.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "3.20.0",
    "react-native-svg": "13.8.0",
    "react-native-vector-icons": "^10.1.0",
    "react-redux": "8.0.5",
    "redux": "4.2.1",
    "redux-flipper": "2.0.2",
    "redux-persist": "6.0.0",
    "rn-async-storage-flipper": "0.0.10"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "7.21.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@react-native/eslint-config": "0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "3.0.0",
    "@types/react": "18.0.28",
    "@types/react-test-renderer": "18.0.0",
    "babel-jest": "29.5.0",
    "eslint": "8.36.0",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": "^14.0.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "2.8.4",
    "react-native-svg-transformer": "1.0.0",
    "react-test-renderer": "18.2.0",
    "sonarqube-scanner": "3.0.1",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "yarn@3.6.4"
}
